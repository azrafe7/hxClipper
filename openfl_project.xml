<?xml version="1.0" encoding="utf-8"?>
<project>
	
	<meta title="ClipperDemo" package="lib.clipper.hx" version="1.0.0" company="azrafe7" />
	<app main="ClipperDemo" path="bin" file="ClipperDemo" />
	<window background="#FFFFFF" fps="60" />
	<window width="1000" height="600" unless="mobile" />
	<window orientation="landscape" vsync="false" antialiasing="0" if="cpp" />
	
	<set name="SWF_VERSION" value="11.2" />
	
	<source path="src" />
	<source path="../../src" />
	<!--haxeflag name="-D" value="USE_INT64" /-->
	<haxeflag name="-D" value="USE_LINES" />
	<!--haxeflag name="-D" value="USE_XYZ" /-->
	<haxeflag name="-D" value="swf-script-timeout=60" />
	<!--haxeflag name="-D" value="next" /-->
	<!--haxeflag name="--no-traces" if="release" /-->
	<haxelib name="openfl" />
	<haxelib name="hx3compat" />
	
</project>